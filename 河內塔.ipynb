        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "def hanoi(n, start, end, other):\n",
        "  if not n:\n",
        "    return\n",
        "  hanoi(n-1, start, other, end)\n",
        "  print(f'move disk from {start} to {end}')\n",
        "  hanoi(n-1, other, end, start)"
      ],
      "execution_count": 0,
      "outputs": []
